@model VaccinationCampaignUI.ViewModels.MedicalInstitutionViewModel;


<form method="post">
    <label>Мед учреждение</label>
    <input type="text" name="NameMedInst" value="@Model.NameMedInst" />
    <br />
    <label>Адрес</label>
    <input type="text" name="AdressMedInst" value="@Model.AdressMedInst" />
    <br />
    <label>Обращение</label>
    <select name="appealId" asp-items="@(new SelectList(Model.Appeal, "Id", "Name"))"></select>
    <br />

    <input type="submit" asp-action="Edit" asp-controller="MedicalInstitution" value="Edit" />
</form>